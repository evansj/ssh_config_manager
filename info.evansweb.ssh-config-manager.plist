<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN
http://www.apple.com/DTDs/PropertyList-1.0.dtd>
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>info.evansweb.ssh-config-manager</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/USERNAME/.ssh/build-config.sh</string>
    </array>
    <key>WatchPaths</key>
    <array>
        <string>/Users/USERNAME/.ssh/config.d/</string>
        <string>/Users/USERNAME/.ssh/build-config.sh</string>
    </array>
    <key>ThrottleInterval</key>
    <integer>1</integer>
    <key>ProcessType</key>
    <string>Background</string>
</dict>
</plist>
